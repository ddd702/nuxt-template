{
  "name": "nuxt-app",
  "private": true,
  "overrides": {
    "vue": "latest"
  },
  "scripts": {
    "build": "nuxt build --mode=production",
    "build:dev": "nuxt build --mode=development",
    "dev": "NUXT_HOST=0.0.0.0 nuxt dev --mode=development",
    "dev:pro": "nuxt dev --mode=production",
    "generate": "nuxt generate --mode=production",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "eslint . --ext .vue,.ts,.tsx --fix --ignore-path .eslintignore"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^18",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nuxt": "^3.4.2",
    "prettier": "^2.8.3",
    "sass": "^1.58.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.6.6",
    "@pinia/nuxt": "^0.5.1",
    "nuxt-icon": "^0.3.3",
    "nuxt-lazy-load": "^3.0.4",
    "pinia": "^2.1.7"
  }
}
